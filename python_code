import numpy as np
nodes=np.genfromtxt('coordinate.txt', delimiter=' ', names=True)
xcord=nodes['x']
ycord=nodes['y']
zcord=nodes['z']
elements=np.genfromtxt('elements.txt', delimiter=' ', names=True)
numelem=len(elements['node1'])
numnodes=len(xcord)
for i in range(1,numelem)
    xcorde(element(i,1))
    ycorde(element(i,1)
    zcord(element(i,1))
#element2
    xcorde(element(i,2))
    ycorde(element(i,2)
    zcord(element(i,2))
#element3
    xcorde(element(i,3))
    ycorde(element(i,3)
    zcord(element(i,3))
#element4
    xcorde(element(i,4))
    ycorde(element(i,4)
    zcord(element(i,4)
print(numnodes)
print(numelem)
