#pragma once


#include"head.h"
#include <vector>
using namespace std;
int main(){
 vector<vector<double>> matrixoperation::matrixdim(int M, int N);
 
     vector<vector<double>> matrix;
     matrix.resize(M);
     
      for( int i=0; i<M; ++i){
        
        matrix[i].resize(N);
      }
return matrix;

vector<vector<double>> matrixoperation::multmatrix(vector<vector<double>> mat1, vector<vector<double>> mat2);
int M=mat1.size();
int N=mat2[0].size();
vector<vector<double>> resultmatrix=matrixdim(int M, int N);
for (i=0; i<M; ++i){
    
    for(int j=0; j<N; ++i){
        
        for (int k=0, k<N; ++k){
            
            double sumelemnts=0;
            sumelements+=mat1[i][k]*mat2[k][j];
        }
    }
}
resultmatrix[i][j]=sumelements;
return resultmatrix;

}

